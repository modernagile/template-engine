#!/bin/sh

APPDIR=$(readlink -f "$0")
APPDIR=$(dirname "$APPDIR")
java -Djna.nosys=true -Djava.library.path="$APPDIR:$APPDIR/lib" -cp "$APPDIR:$APPDIR/lib/templatr.jar:$APPDIR/lib/core.jar:$APPDIR/lib/jogl-all.jar:$APPDIR/lib/gluegen-rt.jar:$APPDIR/lib/jogl-all-natives-linux-armv6hf.jar:$APPDIR/lib/gluegen-rt-natives-linux-armv6hf.jar:$APPDIR/lib/batik-awt-util-1.8.jar:$APPDIR/lib/batik-dom-1.8.jar:$APPDIR/lib/batik-ext-1.8.jar:$APPDIR/lib/batik-svggen-1.8.jar:$APPDIR/lib/batik-util-1.8.jar:$APPDIR/lib/batik-xml-1.8.jar:$APPDIR/lib/svg.jar:$APPDIR/lib/itext.jar:$APPDIR/lib/pdf.jar:$APPDIR/lib/interfascia.jar" templatr "$@"
